#include <bits/stdc++.h>

using namespace std;

int ultimaColuna(int inf[][2]){
	int i=0;
	while(inf[i][0] != -1) i++;
	return i;
}

int main(){
	int n,linha,coluna,l,c,cont;
	cin >>n >>linha >>coluna;
	int matriz[n+2][n+2];
	int giro[n+2][n+2];
	int inf[n*n][2];
	
	memset(inf,-1,sizeof(inf));
	memset(matriz,0,sizeof(matriz));
	memset(giro,0,sizeof(giro));
	cont = 0, l = linha, c = coluna;
	
	for(int i=1; i<n+1; i++)
		for(int j=1; j<n+1; j++)
			cin>> matriz[i][j];
			
	while(giro[linha][coluna] != 4){
		int k = ultimaColuna(inf);
		cout<<"k="<<k<<endl;
		if(inf[k][0] == l && inf[k][1] == c){
			l = inf[k-1][0];
			c = inf[k-1][1];
			inf[k][0] = -1;
			inf[k][1] = -1;
		}
		else {
			inf[k][0] = l;
			inf[k][1] = c;
		}
		
		while(giro[l][c] != 4){
			if(giro[l][c] == 0){
				if(matriz[l][c] > matriz[l-1][c]) giro[l][c]++;
				else{
					l--;
					break;
				}
			}
			if(giro[l][c] == 1){
				if(matriz[l][c] > matriz[l][c+1]) giro[l][c]++;
				else{
					c++;
					break;
				}
			}
			if(giro[l][c] == 2){
				if(matriz[l][c] > matriz[l+1][c]) giro[l][c]++;
				else{
					l++;
					break;
				}
			}
			if(giro[l][c] == 3){
				if(matriz[l][c] > matriz[l][c-1]) giro[l][c]++;
				else{
				    c--;
					break;
				}
			}
		}
	}
	
	for(int i=0; i<n+2; i++){
		for(int j=0; j<n+2; j++)
			cout<<giro[i][j]<<" ";
		cout<<endl;
	}
	
	return 0;
}
