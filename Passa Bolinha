#include <bits/stdc++.h>

using namespace std;

int ultimaColuna(int inf[][2]){
	int i=0;
	while(inf[i][0] != -1) i++;
	return i;
}

void imprimir(int m[][4]){
	for(int i=0; i<4 ; i++){
		for(int j=0; j<4 ; j++)
			cout<<m[i][j]<<" ";
		cout<<endl;
	}
	cout<<"---------------"<<endl;
}
void imprimir(int m[][2]){
	for(int i=0; i<4 ; i++){
		for(int j=0; j<2 ; j++)
			cout<<m[i][j]<<" ";
		cout<<endl;
	}
	cout<<"---------------"<<endl;
}



int main(){
	int n,linha,coluna,l,c,k,cont;
	cin >>linha >>coluna;
	int matriz[4][4];
	int giro[4][4];
	int inf[4][2];
	
	memset(inf,-1,sizeof(inf));
	memset(matriz,0,sizeof(matriz));
	memset(giro,0,sizeof(giro));
	cont = 0, l = linha, c = coluna;
	
	for(int i=1; i<2+1; i++)
		for(int j=1; j<2+1; j++)
			cin>> matriz[i][j];
	
	imprimir(matriz);
	int teste=0;
	while(teste<5 && giro[linha][coluna] != 4){
		if(giro[l][c] == 4){
			l = inf[k-1][0];
			c = inf[k-1][1];
			inf[k][0] = -1;
			inf[k][1] = -1;
		}
		else {
			k = ultimaColuna(inf);
			inf[k][0] = l;
			inf[k][1] = c;
		}
		cout<<l<<" "<<c<<endl;
		imprimir(inf);
		teste++;
		
		while(giro[l][c] != 4){
			cout<<"X";
			teste=0;
			if(giro[l][c] == 0){
				if(matriz[l][c] > matriz[l-1][c]) giro[l][c]++;
				else{
					giro[l][c]++;
					l--;
					break;
				}
			}
			if(giro[l][c] == 1){
				if(matriz[l][c] > matriz[l][c+1]) giro[l][c]++;
				else{
					giro[l][c]++;
					c++;
					break;
				}
			}
			if(giro[l][c] == 2){
				if(matriz[l][c] > matriz[l+1][c]) giro[l][c]++;
				else{
					giro[l][c]++;
					l++;
					break;
				}
			}
			if(giro[l][c] == 3){
				if(matriz[l][c] > matriz[l][c-1]) giro[l][c]++;
				else{
					giro[l][c]++;
					c--;
					break;
				}
			}
		}
	}
	
	imprimir(giro);
	
	return 0;
}
